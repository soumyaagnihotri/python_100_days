import math
def prime_check( n)->bool:
    is_prime=False
    for i in range(2,round(math.sqrt(n))):
        if(n%i==0):
            is_prime=True
    if is_prime == False:
        print(f"{n} is a prime number")
    else:
        print(f"{n} is not a prime number")
num=int(input("Check this number "))
prime_check(num)
